{
  "name": "agentic",
  "version": "1.0.0",
  "private": true,
  "description": "Agentic - Production-ready AI agent framework with stateless architecture and multi-LLM support",
  "pnpm": {
    "overrides": {
      "glob": ">=10.5.0",
      "@isaacs/brace-expansion": ">=5.0.1",
      "esbuild": ">=0.25.0"
    },
    "auditConfig": {
      "ignoreCves": [
        "CVE-2026-0775"
      ]
    }
  },
  "scripts": {
    "build": "pnpm -r --workspace-concurrency=1 --filter='./packages/*' run build",
    "clean": "pnpm -r --filter='./packages/*' run clean",
    "test": "pnpm -r --filter='./packages/*' run test",
    "test:coverage": "pnpm -r --filter='./packages/*' run test:coverage",
    "typecheck": "pnpm -r --filter='./packages/*' run typecheck",
    "lint": "pnpm -r --filter='./packages/*' run lint",
    "lint:fix": "pnpm -r --filter='./packages/*' run lint:fix",
    "format": "pnpm -r --filter='./packages/*' run format",
    "format:check": "pnpm -r --filter='./packages/*' run format:check"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "framework",
    "rag",
    "tool-calling"
  ],
  "author": "Chajian <xylyjy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
