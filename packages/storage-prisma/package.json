{
  "name": "@ai-agent/storage-prisma",
  "version": "1.0.0",
  "type": "module",
  "description": "Prisma storage adapter for @ai-agent/core - production-ready SQL database storage",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "prisma",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "agent",
    "storage",
    "prisma",
    "database",
    "sql",
    "postgresql",
    "mysql",
    "sqlite",
    "session",
    "conversation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Chajian/ai-agent-framework.git"
  },
  "bugs": {
    "url": "https://github.com/Chajian/ai-agent-framework/issues"
  },
  "homepage": "https://github.com/Chajian/ai-agent-framework#readme",
  "author": "Chajian <936796603@qq.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate",
    "prepublishOnly": "npm run clean && npm run prisma:generate && npm run build && npm run typecheck"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "prisma": "^6.1.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@ai-agent/core": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
